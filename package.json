{
  "private": true,
  "name": "insomnia-plugin-free-sync",
  "version": "0.1.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Nick Rimmer <xan@dipteam.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/NickRimmer/insomnia-plugin-free-sync.git",
    "directory": ""
  },
  "bugs": {
    "url": "https://github.com/NickRimmer/insomnia-plugin-free-sync/issues"
  },
  "scripts": {
    "build": "esr esbuild.ts",
    "clean": "rimraf dist",
    "watch": "npm-watch build",
    "publish": "node scripts/npm-publish.js && copyfiles -u 1 ./npm_content/* ./dist/ && cd dist && npm publish",
    "publish-dry": "node scripts/npm-publish.js && copyfiles -u 1 ./npm_content/* ./dist/ && cd dist && npm publish --dry-run"
  },
  "insomnia": {
    "name": "free-sync",
    "displayName": "Free sync",
    "description": "Allow to read/write workspace settings to a file",
    "unlisted": true,
    "images": {
      "icon": "free-sync-icon-boxed.svg"
    },
    "publisher": {
      "name": "Nick Rimmer",
      "icon": "https://www.gravatar.com/avatar/53a530a44db8d365a66bcdef9d49e23c"
    }
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "ts,tsx,scss"
    }
  },
  "devDependencies": {
    "@types/nedb": "^1.8.11",
    "@types/node": "^18.11.18",
    "@types/react": "^16.14.5",
    "@types/react-dom": "^16.9.12",
    "@types/uuid": "^9.0.0",
    "copyfiles": "^2.4.1",
    "electron": "22.0.0",
    "esbuild": "^0.14.29",
    "esbuild-runner": "^2.2.1",
    "esbuild-sass-plugin": "^2.4.5",
    "nedb": "^1.8.0",
    "node-sass": "^8.0.0",
    "npm-watch": "^0.11.0",
    "postcss": "^8.4.20",
    "postcss-parent-selector": "^1.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "request": "^2.88.2",
    "rimraf": "3.0.2",
    "sass": "^1.57.1",
    "semver": "^7.3.8",
    "typescript": "^4.5.5",
    "uuid": "^8.3.2"
  }
}
